@using TravelWebAppInMVC.Models.Classes
@model List<About>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
<div class="content">
    <div class="wrap">
        <div class="about">
            <h3>About</h3>
            @foreach (var item in Model)
            {
                <img src="@item.PhotoUrl" alt="" />
                <p>@item.Contents</p>
            }
        </div>
    </div>
</div>